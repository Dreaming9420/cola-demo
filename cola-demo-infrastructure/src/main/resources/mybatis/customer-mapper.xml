<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.infrastructure.customer.gatewayimpl.database.mapper.CustomerMapper">
    <select id="getById" resultType="org.example.infrastructure.customer.gatewayimpl.database.dataobject.CustomerDO">
        select *
        from customer
        where customer_id = #{id}
    </select>
</mapper>
